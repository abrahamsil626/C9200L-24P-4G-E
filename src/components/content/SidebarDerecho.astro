---
import producto from '../../config/descripcion_cisco_C9200L-24P-4G-E.json';

// Como es un array con un solo item:
const prod = producto[0];
---


<!-- CONTENT DE 3 COMULNAS SIDEBAR DERECHO - IZQUIERDO Y INFORMACION DE PRODUCTO -->
    <div class="mx-auto border">
      <div class="flex flex-col md:flex-row md:justify-evenly  mx-auto">


        <!-- SIDEBAR DE MARCAS (oculto en móvil) -->
        <aside
          class="hidden lg:block bg-white p-2 rounded-lg shadow-md w-full md:w-48 lg:w-56 h-[80vh] overflow-y-auto scrollbar-hide">
          <!-- Título -->
          <h2 class="text-lg font-bold text-gray-800 mb-4 pb-2 border-b">
            Nuestras Marcas
          </h2>

          <!-- Contenedor de marcas - responsive -->
          <div class="grid grid-cols-2 sm:grid-cols-3 md:grid-cols-1 ">
            <!-- Marca Alfa -->
            <a href="https://www.ds3comunicaciones.com/alfa/index.html"
              class="flex items-center justify-center  rounded-md hover:bg-gray-100 transition-all hover:-translate-y-0.5">
              <img src="http://www.ds3comunicaciones.com/belden/images/ALFA_logo1.gif" alt="Alfa"
                class="max-w-[100px] max-h-8 w-auto h-auto object-contain" />
            </a>

            <!-- Marca Airlive -->
            <a href="https://www.ds3comunicaciones.com/airlive/index.html"
              class="flex items-center justify-center p-1 rounded-md hover:bg-gray-100 transition-all hover:-translate-y-0.5">
              <img src="http://www.ds3comunicaciones.com/belden/images/airlivelogo.gif" alt="Airlive"
                class="max-w-[100px] max-h-8 w-auto h-auto object-contain" />
            </a>

            <!-- Marca Andrew -->
            <a href="https://www.ds3comunicaciones.com/andrew/index.html"
              class="flex items-center justify-center p-1 rounded-md hover:bg-gray-100 transition-all hover:-translate-y-0.5">
              <img src="http://www.ds3comunicaciones.com/andrew/images/logo_andrew.jpg" alt="Andrew"
                class="max-w-[100px] max-h-8 w-auto h-auto object-contain" />
            </a>

            <!-- Marca AMP -->
            <a href="https://www.ds3comunicaciones.com/AMP/index.html"
              class="flex items-center justify-center p-1 rounded-md hover:bg-gray-100 transition-all hover:-translate-y-0.5">
              <img src="http://www.ds3comunicaciones.com/belden/images/amp_logo2-91x20.jpg" alt="AMP"
                class="max-w-[100px] max-h-8 w-auto h-auto object-contain" />
            </a>

            <!-- Marca Amphenol -->
            <a href="https://www.ds3comunicaciones.com/amphenol/index.html"
              class="flex items-center justify-center p-1 rounded-md hover:bg-gray-100 transition-all hover:-translate-y-0.5">
              <img src="http://www.ds3comunicaciones.com/belden/images/amphenol_logo-100x21.jpg" alt="Amphenol"
                class="max-w-[100px] max-h-8 w-auto h-auto object-contain" />
            </a>

            <!-- Marca Belden -->
            <a href="https://www.ds3comunicaciones.com/belden/index.html"
              class="flex items-center justify-center p-1 rounded-md hover:bg-gray-100 transition-all hover:-translate-y-0.5">
              <img src="http://www.ds3comunicaciones.com/belden/images/Belden-Logo-93x15.jpg" alt="Belden"
                class="max-w-[100px] max-h-8 w-auto h-auto object-contain" />
            </a>

            <!-- Marca Cisco -->
            <a href="https://www.ds3comunicaciones.com/cisco/index.html"
              class="flex items-center justify-center p-1 rounded-md hover:bg-gray-100 transition-all hover:-translate-y-0.5">
              <img src="http://www.ds3comunicaciones.com/cisco/images/logo-cisco.gif" alt="Cisco"
                class="max-w-[100px] max-h-8 w-auto h-auto object-contain" />
            </a>

            <!-- Marca Dahua -->
            <a href="https://www.ds3comunicaciones.com/dahua/index.html"
              class="flex items-center justify-center p-1 rounded-md hover:bg-gray-100 transition-all hover:-translate-y-0.5">
              <img src="http://www.ds3comunicaciones.com/dahua/images/dahua_logo.jpg" alt="Dahua"
                class="max-w-[100px] max-h-8 w-auto h-auto object-contain" />
            </a>

            <!-- Marca D-Link -->
            <a href="https://www.ds3comunicaciones.com/dlink/index.html"
              class="flex items-center justify-center p-1 rounded-md hover:bg-gray-100 transition-all hover:-translate-y-0.5">
              <img src="http://www.ds3comunicaciones.com/belden/images/D-Link_Logo-100x26.jpg" alt="D-Link"
                class="max-w-[100px] max-h-8 w-auto h-auto object-contain" />
            </a>

            <!-- Marca Dixon -->
            <a href="https://www.ds3comunicaciones.com/dixon/index.html"
              class="flex items-center justify-center p-1 rounded-md hover:bg-gray-100 transition-all hover:-translate-y-0.5">
              <img src="http://www.ds3comunicaciones.com/belden/images/dixon_logo-100x19.jpg" alt="Dixon"
                class="max-w-[100px] max-h-8 w-auto h-auto object-contain" />
            </a>

            <!-- Marca FibreFab -->
            <a href="https://www.ds3comunicaciones.com/FibreFab/index.html"
              class="flex items-center justify-center p-1 rounded-md hover:bg-gray-100 transition-all hover:-translate-y-0.5">
              <img src="http://www.ds3comunicaciones.com/FibreFab/images/logo_fibrefab.png" alt="FibreFab"
                class="max-w-[100px] max-h-8 w-auto h-auto object-contain" />
            </a>

            <!-- Marca LanPro -->
            <a href="https://www.ds3comunicaciones.com/lanpro/index.html"
              class="flex items-center justify-center p-1 rounded-md hover:bg-gray-100 transition-all hover:-translate-y-0.5">
              <img src="http://www.ds3comunicaciones.com/lanpro/images/lanpro_logo.jpg" alt="LanPro"
                class="max-w-[100px] max-h-8 w-auto h-auto object-contain" />
            </a>

            <!-- Marca L-Com -->
            <a href="https://www.ds3comunicaciones.com/l-com/"
              class="flex items-center justify-center p-1 rounded-md hover:bg-gray-100 transition-all hover:-translate-y-0.5">
              <img src="http://www.ds3comunicaciones.com/sitebuilder/images/lcom_logo-100x23.jpg" alt="L-Com"
                class="max-w-[100px] max-h-8 w-auto h-auto object-contain" />
            </a>

            <!-- Marca MikroTik -->
            <a href="https://www.ds3comunicaciones.com/mikrotik/index.html"
              class="flex items-center justify-center p-1 rounded-md hover:bg-gray-100 transition-all hover:-translate-y-0.5">
              <img src="http://www.ds3comunicaciones.com/belden/images/mikrotik_logo-100x24.png" alt="MikroTik"
                class="max-w-[100px] max-h-8 w-auto h-auto object-contain" />
            </a>

            <!-- Marca Panduit -->
            <a href="https://www.ds3comunicaciones.com/panduit/index.html"
              class="flex items-center justify-center p-1 rounded-md hover:bg-gray-100 transition-all hover:-translate-y-0.5">
              <img src="http://www.ds3comunicaciones.com/panduit/images/panduit_logo.jpg" alt="Panduit"
                class="max-w-[100px] max-h-8 w-auto h-auto object-contain" />
            </a>

            <!-- Marca Satra -->
            <a href="https://www.ds3comunicaciones.com/satra/index.html"
              class="flex items-center justify-center p-1 rounded-md hover:bg-gray-100 transition-all hover:-translate-y-0.5">
              <img src="http://www.ds3comunicaciones.com/satra/images/satra.JPG" alt="Satra"
                class="max-w-[100px] max-h-8 w-auto h-auto object-contain" />
            </a>

            <!-- Marca Siemon -->
            <a href="https://www.ds3comunicaciones.com/siemon/index.html"
              class="flex items-center justify-center p-1 rounded-md hover:bg-gray-100 transition-all hover:-translate-y-0.5">
              <img src="http://www.ds3comunicaciones.com/siemon/images/siemon_logo1.GIF" alt="Siemon"
                class="max-w-[100px] max-h-8 w-auto h-auto object-contain" />
            </a>

            <!-- Marca TP-Link -->
            <a href="https://www.ds3comunicaciones.com/tplink/"
              class="flex items-center justify-center p-1 rounded-md hover:bg-gray-100 transition-all hover:-translate-y-0.5">
              <img src="http://www.ds3comunicaciones.com/belden/images/tp-link_logo-100x13.jpg" alt="TP-Link"
                class="max-w-[100px] max-h-8 w-auto h-auto object-contain" />
            </a>

            <!-- Marca Trendnet -->
            <a href="https://www.ds3comunicaciones.com/trendnet/"
              class="flex items-center justify-center p-1 rounded-md hover:bg-gray-100 transition-all hover:-translate-y-0.5">
              <img src="http://www.ds3comunicaciones.com/belden/images/trendnet_logo-100x20.jpg" alt="Trendnet"
                class="max-w-[100px] max-h-8 w-auto h-auto object-contain" />
            </a>

            <!-- Marca Ubiquiti -->
            <a href="https://www.ds3comunicaciones.com/ubiquiti/index.html"
              class="flex items-center justify-center p-1 rounded-md hover:bg-gray-100 transition-all hover:-translate-y-0.5">
              <img src="http://www.ds3comunicaciones.com/belden/images/ubnt_logo-100x47.png" alt="Ubiquiti"
                class="max-w-[100px] max-h-8 w-auto h-auto object-contain" />
            </a>
          </div>
        </aside>


        <!-- CONTENIDO PRINCIPAL DE PRODUCTOS (siempre visible) -->
        <div class="w-full lg:w-4/6 bg-white  rounded-lg shadow">
          <div class="mx-auto max-w-[1200px] w-full">

            <!-- Contenedor principal del producto -->
            <div id="information-section" class="mt-5 flex flex-col lg:flex-row gap-6 flex-wrap">
              <!-- Sección de información  e imagenes-->
              <div class="w-full flex flex-col lg:flex-row gap-6">

                <!-- Sección de imágenes -->
                <div class="lg:w-1/2">
                  <!-- Imagen principal -->
                  <div class="bg-white rounded-lg shadow-sm p-4">
                    <div class="w-full h-[400px] flex items-center justify-center overflow-hidden">
                      <img id="img_main"
                        src={prod.ruta_imagenes[0].source}
                        alt="Switch Catalyst 2960" class="max-h-full max-w-full object-contain cursor-pointer"
                        onclick="viewImage(this.src)" />
                    </div>

                    <!-- Miniaturas -->
                    <div class="flex gap-3 mt-4 justify-center">
                      {
                        prod.ruta_imagenes.map((img, i) => (
                          <div class="w-20 h-20 border rounded p-1 cursor-pointer hover:border-blue-400 transition">
                            <img
                              src={img.source}
                              alt={`Vista ${i + 1}`}
                              class="w-full h-full object-contain"
                              onclick="toExchangeImage(this)"
                            />
                          </div>
                        ))
                      }
                    </div>
                  </div>
                </div>

                <!-- Sección de información -->
                <div class="lg:w-full px-1">
                  <div class="bg-white rounded-lg shadow-sm">
                    <h1 class="md:text-md font-bold text-black mb-2 ">{prod.nombre_producto}</h1>
                    <div class="mb-6">
                      <span class="text-2xl font-bold text-blue-600">U$ {prod.precio_producto}</span>
                      <a href={prod.link_compra}
                        class="mt-3 inline-flex items-center gap-1 bg-blue-600 text-white font-medium rounded-md px-4 py-2 hover:bg-blue-700 transition"
                        target="_blank">
                        <i class="fas fa-shopping-cart"></i>
                        Comprar ahora
                      </a>
                    </div>

                    <div class="border-t pt-4">
                      <h3 class="font-semibold text-md mb-3">
                        Especificaciones técnicas:
                      </h3>
                      <ul class="space-y-2 text-gray-700">
                        {prod.lista_especificaciones_tecnicas.map((esp) => (
                          <li class="flex items-start text-[#0D274D]">
                            <i class="fa-solid fa-circle text-[10px] text-[#0D274D] mt-1.5 mr-2 text"></i>
                            <strong>{esp.parametro}:</strong>{esp.valor}
                          </li>
                        ))}
                      </ul>
                    </div>

                    <div class="mt-6">
                      <h3 class="font-semibold text-lg mb-3">
                        Download:
                      </h3>
                      <div class="flex flex-wrap gap-3">
                        <!-- Ficha técnica PDF -->
                        <a href={prod.ficha_tecnica_pdf}
                          class="flex items-center gap-2 text-blue-600 hover:text-blue-800 border border-gray-200 rounded-md px-4 py-2 hover:bg-gray-50 transition"
                          target="_blank" rel="noopener noreferrer" aria-label="Descargar ficha técnica en PDF">
                          <i class="fas fa-file-pdf text-red-500" aria-hidden="true"></i>
                          <span>Ficha técnica</span>
                        </a>

                        <!-- Manual PDF -->
                        <a href={prod.manual_pdf}
                          class="flex items-center gap-2 text-blue-600 hover:text-blue-800 border border-gray-200 rounded-md px-4 py-2 hover:bg-gray-50 transition"
                          target="_blank" rel="noopener noreferrer" aria-label="Descargar manual en PDF">
                          <i class="fas fa-book-open text-blue-500" aria-hidden="true"></i>
                          <span>Manual</span>
                        </a>

                        <!-- Especificaciones PDF  a -->
                        <a href={prod.especificaciones_pdf}
                          class="flex items-center gap-2 text-blue-600 hover:text-blue-800 border border-gray-200 rounded-md px-4 py-2 hover:bg-gray-50 transition"
                          target="_blank" rel="noopener noreferrer" aria-label="Descargar especificaciones en PDF">
                          <i class="fas fa-file-alt text-green-500" aria-hidden="true"></i>
                          <span>Especificaciones</span>
                        </a>
                      </div>
                    </div>
                  </div>
                </div>
              </div>
            </div>


            <!-- Sección de galería de imágenes (oculta inicialmente) -->
            <div id="images-section" class="hidden mt-6">
              <div class="grid grid-cols-1 md:grid-cols-2 lg:grid-cols-3 gap-4">
                <div class="bg-white rounded-lg shadow-sm overflow-hidden">
                  <img src="https://web.netperu100.com/cisco/catalyst/images/C9200L-24T-4G-E_front.jpg"
                    alt="Vista frontal"
                    class="w-full h-64 object-contain p-4 cursor-pointer hover:opacity-90 transition"
                    onclick="viewImage(this.src)" />
                </div>
                <div class="bg-white rounded-lg shadow-sm overflow-hidden">
                  <img src="https://web.netperu100.com/cisco/catalyst/images/C9200L-24T-4G-E_front_large.jpg"
                    alt="Vista frontal ampliada"
                    class="w-full h-64 object-contain p-4 cursor-pointer hover:opacity-90 transition"
                    onclick="viewImage(this.src)" />
                </div>
                <div class="bg-white rounded-lg shadow-sm overflow-hidden">
                  <img src="https://web.netperu100.com/cisco/catalyst/images/C9200L-24T-4G-E_back.jpg"
                    alt="Vista posterior"
                    class="w-full h-64 object-contain p-4 cursor-pointer hover:opacity-90 transition"
                    onclick="viewImage(this.src)" />
                </div>
              </div>
            </div>


            <!-- Controles de pestañas -->
            <div class="flex justify-center mt-6">
              <div class="inline-flex rounded-md shadow-sm">
                <button id="product-tab"
                  class="px-6 py-2 text-xs md:text-sm font-medium rounded-l-lg bg-blue-600 text-white focus:z-10 focus:outline-none">
                  Producto
                </button>
                <button id="images-tab"
                  class="px-6 py-2 text-xs md:text-sm font-medium rounded-r-lg bg-gray-100 text-gray-700 focus:z-10 focus:outline-none">
                  Imágenes
                </button>
              </div>
            </div>



            <!-- desplegables de informacion -->
            <div class="mt-8 w-full mx-auto space-y-4">

              <!-- Especificaciones Técnicas -->
              <details open class="group border border-gray-200 rounded-lg overflow-hidden shadow-sm">
                <summary
                  class="flex items-center justify-between p-4 bg-gray-50 hover:bg-gray-100 cursor-pointer transition-colors">
                  <h3 class="font-semibold text-black flex items-center">
                    <svg class="w-5 h-5 mr-2 text-blue-600 transition-transform group-open:rotate-90" fill="none"
                      stroke="currentColor" viewBox="0 0 24 24">
                      <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 5l7 7-7 7"></path>
                    </svg>
                    Especificaciones Técnicas
                  </h3>
                  <span class="text-blue-600 text-xs text-nowrap font-medium">Ver detalles</span>
                </summary>

                <div class="p-4 bg-white grid grid-cols-1 md:grid-cols-2 gap-4">
                  
                  
                  {prod.especificaciones_tecnicas_desplegable.map((esp) => (
                    <div class="flex items-start">
                      <div class="shrink-0 h-5 w-5 text-blue-500 mt-0.5">
                        <svg fill="none" stroke="currentColor" viewBox="0 0 24 24">
                          <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M5 13l4 4L19 7"></path>
                        </svg>
                      </div>
                      <div class="ml-2">
                        <p class="text-gray-700">
                          <span class="font-medium">{esp.parametro}</span>
                          {esp.valor}
                        </p>
                      </div>
                    </div>        
                  ))}

                  <!-- Más especificaciones... -->
                </div>

              </details>

              <!-- Protocolos Soportados -->
              <details class="group border border-gray-200 rounded-lg overflow-hidden shadow-sm">
                <summary
                  class="flex items-center justify-between p-4 bg-gray-50 hover:bg-gray-100 cursor-pointer transition-colors">
                  <h3 class="font-semibold text-black flex items-center">
                    <svg class="w-5 h-5 mr-2 text-blue-600 transition-transform group-open:rotate-90" fill="none"
                      stroke="currentColor" viewBox="0 0 24 24">
                      <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 5l7 7-7 7"></path>
                    </svg>
                    Protocolos Soportados
                  </h3>
                  <span class="text-blue-600 text-xs text-nowrap font-medium">Ver detalles</span>
                </summary>
                <div class="p-4 bg-white">
                  <div class="space-y-3">
                    <div class="flex items-start">
                      <div class="shrink-0 h-5 w-5 text-blue-500 mt-0.5">
                        <svg fill="none" stroke="currentColor" viewBox="0 0 24 24">
                          <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M5 13l4 4L19 7">
                          </path>
                        </svg>
                      </div>
                      <div class="ml-2">
                        <p class="text-gray-700">
                          <span class="font-medium">Protocolo de direccionamiento:</span>
                          {producto[0]?.lista_especificaciones_tecnicas?.find(item => item.parametro === "Soporta")?.valor ?? ""}
                        </p>
                      </div>
                    </div>
                    <!-- Más protocolos... -->
                  </div>
                </div>
              </details>

              <!-- Cumplimiento de Normas -->
              <details class="group border border-gray-200 rounded-lg overflow-hidden shadow-sm">
                <summary
                  class="flex items-center justify-between p-4 bg-gray-50 hover:bg-gray-100 cursor-pointer transition-colors">
                  <h3 class="font-semibold text-black flex items-center">
                    <svg class="w-5 h-5 mr-2 text-blue-600 transition-transform group-open:rotate-90" fill="none"
                      stroke="currentColor" viewBox="0 0 24 24">
                      <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 5l7 7-7 7"></path>
                    </svg>
                    Cumplimiento de Normas
                  </h3>
                  <span class="text-blue-600 text-xs text-nowrap font-medium">Ver detalles</span>
                </summary>
                <div class="p-4 bg-white">
                  <div class="flex items-start">
                    <div class="shrink-0 h-5 w-5 text-blue-500 mt-0.5">
                      <svg fill="none" stroke="currentColor" viewBox="0 0 24 24">
                        <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M5 13l4 4L19 7"></path>
                      </svg>
                    </div>
                    <div class="ml-2">
                      <p class="text-gray-700">
                        <!-- { "parametro": "Normas de Cumplimiento", "valor": "TIA/EIA-568-B.2-1, ISO/IEC 11801 Clase E" } -->
                        {producto[0]?.lista_especificaciones_tecnicas?.find(item => item.parametro === "Normas de Cumplimiento")?.valor ?? ""}
                      </p>
                    </div>
                  </div>
                </div>
              </details>

              <!-- Certificaciones y Ambiente -->
              <details class="group border border-gray-200 rounded-lg overflow-hidden shadow-sm">
                <summary
                  class="flex items-center justify-between p-4 bg-gray-50 hover:bg-gray-100 cursor-pointer transition-colors">
                  <h3 class="font-semibold text-black flex items-center">
                    <svg class="w-5 h-5 mr-2 text-blue-600 transition-transform group-open:rotate-90" fill="none"
                      stroke="currentColor" viewBox="0 0 24 24">
                      <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 5l7 7-7 7"></path>
                    </svg>
                    Certificaciones y Ambiente
                  </h3>
                  <span class="text-blue-600 text-xs text-nowrap font-medium">Ver detalles</span>
                </summary>
                <div class="p-4 bg-white grid grid-cols-1 md:grid-cols-2 gap-4">
                  <div class="flex items-start">
                    <div class="shrink-0 h-5 w-5 text-blue-500 mt-0.5">
                      <svg fill="none" stroke="currentColor" viewBox="0 0 24 24">
                        <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M5 13l4 4L19 7"></path>
                      </svg>
                    </div>
                    <div class="ml-2">
                      <p class="text-gray-700">
                        <span class="font-medium">{producto[0]?.lista_especificaciones_tecnicas?.find(item => item.parametro === "Clasificación de Fuego")?.parametro ?? ""}</span>:
                        {producto[0]?.lista_especificaciones_tecnicas?.find(item => item.parametro === "Clasificación de Fuego")?.valor ?? ""}
                      </p>
                    </div>
                  </div>
                  <div class="flex items-start">
                    <div class="shrink-0 h-5 w-5 text-blue-500 mt-0.5">
                      <svg fill="none" stroke="currentColor" viewBox="0 0 24 24">
                        <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M5 13l4 4L19 7"></path>
                      </svg>
                    </div>
                    <div class="ml-2">
                      <p class="text-gray-700">
                        <span class="font-medium"><span class="font-medium">{producto[0]?.lista_especificaciones_tecnicas?.find(item => item.parametro === "Certificaciones y Seguridad")?.parametro ?? ""}</span>:</span>
                        {producto[0]?.lista_especificaciones_tecnicas?.find(item => item.parametro === "Certificaciones y Seguridad")?.valor ?? ""}
                      </p>
                    </div>
                  </div>
                </div>
              </details>
            </div>









            
          </div>




          




          <!-- Modal para imágenes -->
          <div id="image-modal"
            class="fixed hidden inset-0 bg-black bg-opacity-90 z-50 flex items-center justify-center p-4">
            <span class="absolute top-4 right-4 text-white text-3xl cursor-pointer"
              onclick="closeModal()">&times;</span>
            <img id="modal-image" class="max-w-full max-h-full" src="" alt="Imagen ampliada" />
          </div>
        </div>

        <!-- SIDEBAR DE LISTA DE PRECIOS (oculto en móvil) -->
        <aside id="links-sidebar"
          class="hidden lg:block w-full md:w-1/5 lg:w-1/6 bg-white p-2 rounded-lg shadow-md h-[80vh] overflow-y-auto scrollbar-hide">
          <div class="links-section bg-white">
            <h3 class="text-base font-bold text-black mb-3 border-b pb-2">
